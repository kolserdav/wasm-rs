{
    "env": {
    "es2021": true,
              "node": false,
                  "browser": true
                    },
                      "parser": "@babel/eslint-parser",
                        "extends": ["eslint:recommended", "eslint-config-prettier", "prettier"],
                          "plugins": ["prettier", "eslint-plugin-prettier", "eslint-plugin-html"],
                            "parserOptions": {
                                  "ecmaVersion": 12,
                                      "sourceType": "module",
                                          "allowImportExportEverywhere": true
                                            },
                                              "ignorePatterns": ["node_modules", "dist"],
                                                "rules": {
                                                      "no-unused-vars": 1,
                                                          "prettier/prettier": 1
                                                            }
}
